{
  "AWSEBDockerrunVersion": 2,
  "containerDefinition": [
    {
    "name": "client",
    "image": "n4g1b4t0r/multi-client",
    "hostname": "client",
    "essential": false
    },
    {
      "name": "server",
      "image": "n4g1b4t0r/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "n4g1b4t0r/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "n4g1b4t0r/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}